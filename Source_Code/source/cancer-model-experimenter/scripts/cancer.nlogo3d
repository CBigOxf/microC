   foreach list-of-cells
           [ let list-item
                 [ my-id ] of first [ my-nodes with [ my-active = true ] ] of objects with [ who = ? ]
             set list-of-nodes
                 lput sort list-item list-of-nodes ]
   report ( list-of-nodes )
end

